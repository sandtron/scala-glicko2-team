<configuration debug="false"> 

 <appender name="FILE" class="ch.qos.logback.core.FileAppender">
     <file>glicko2team.log</file>
        <append>true</append>
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36}:%line - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="trace">
    <appender-ref ref="FILE" />
  </root>
</configuration>